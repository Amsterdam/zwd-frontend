{
  "name": "zwd-frontend",
  "private": true,
  "version": "1.0.0",
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "scripts": {
    "start": "vite",
    "ont": "env-cmd -f .env.ontwikkel vite",
    "acc": "env-cmd -f .env.acceptatie vite",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --fix --ext .ts,.tsx src",
    "prettier": "prettier --check src",
    "prettier:fix": "prettier --write src",
    "swagger:generate-schema": "TS_NODE_PROJECT=./scripts/tsconfig.json ts-node scripts/src/generateSwaggerSchema.ts",
    "test": "vitest",
    "preview": "vite preview"
  },
  "dependencies": {
    "@amsterdam/design-system-react": "^1.1.0",
    "@amsterdam/design-system-react-icons": "^0.1.13",
    "@types/lodash.debounce": "^4.0.9",
    "@types/qs": "^6.14.0",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "axios": "^1.9.0",
    "bpmn-js": "^18.6.1",
    "dayjs": "^1.11.13",
    "immer": "^10.1.1",
    "jspdf": "^3.0.1",
    "jwt-decode": "^4.0.0",
    "lodash.debounce": "^4.0.8",
    "qs": "^6.14.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.56.3",
    "react-oidc-context": "^3.3.0",
    "react-router-dom": "^7.6.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "vite": "^6.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "dtsgenerator": "^3.19.2",
    "env-cmd": "^10.1.0",
    "eslint": "^9.26.0",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^16.1.0",
    "prettier": "^3.5.0",
    "typescript-eslint": "^8.32.0",
    "vitest": "^3.1.3"
  },
  "overrides": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
